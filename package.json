{
  "name": "swa",
  "version": "1.0.0",
  "description": "A high-level Steam Web API wrapper for Node.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess \"npm run test\"",
    "clean": "rimraf dist",
    "test": "node test/index.test.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antilmid/swa.git"
  },
  "keywords": [
    "steam",
    "api",
    "web-api",
    "wrapper",
    "nodejs",
    "typescript"
  ],
  "author": "antilmid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antilmid/swa/issues"
  },
  "homepage": "https://github.com/antilmid/swa#readme",
  "dependencies": {
    "axios": "^1.13.4"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
